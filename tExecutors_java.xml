		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="PARALLELIZE" MAX_INPUT="1" COLOR="575DB1" />
