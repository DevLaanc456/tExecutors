				<ITEM NAME="FIXED_DELAY" VALUE="FIXED_DELAY" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="TIME_UNIT" FIELD="CLOSED_LIST" NUM_ROW="30"
			SHOW_IF="(THREAD_POOL_TYPE=='SCHEDULED') AND ((SCHEDULED_TYPE=='NORMAL') OR (SCHEDULED_TYPE=='FIXED_RATE') OR (SCHEDULED_TYPE=='FIXED_DELAY'))">
			<ITEMS DEFAULT="SECONDS">
				<ITEM NAME="DAYS" VALUE="DAYS" />
				<ITEM NAME="HOURS" VALUE="HOURS" />
				<ITEM NAME="MICROSECONDS" VALUE="MICROSECONDS" />
				<ITEM NAME="MILLISECONDS" VALUE="MILLISECONDS" />
				<ITEM NAME="MINUTES" VALUE="MINUTES" />
				<ITEM NAME="NANOSECONDS" VALUE="NANOSECONDS" />
				<ITEM NAME="SECONDS" VALUE="SECONDS" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="INITIAL_DELAY" FIELD="TEXT" NUM_ROW="50"
			SHOW_IF="(THREAD_POOL_TYPE=='SCHEDULED') AND ((SCHEDULED_TYPE=='FIXED_RATE') OR (SCHEDULED_TYPE=='FIXED_DELAY'))">
			<DEFAULT>1</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="DELAY" FIELD="TEXT" NUM_ROW="50"
			SHOW_IF="(THREAD_POOL_TYPE=='SCHEDULED') AND ((SCHEDULED_TYPE=='NORMAL') OR (SCHEDULED_TYPE=='FIXED_DELAY'))">
			<DEFAULT>1</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PERIOD" FIELD="TEXT" NUM_ROW="50"
			SHOW_IF="(THREAD_POOL_TYPE=='SCHEDULED') AND (SCHEDULED_TYPE=='FIXED_RATE')">
			<DEFAULT>1</DEFAULT>
		</PARAMETER>
	</PARAMETERS>
	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="TIMEOUT_UNIT" FIELD="CLOSED_LIST" NUM_ROW="10"
