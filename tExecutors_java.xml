	<PARAMETERS>
		<PARAMETER NAME="THREAD_POOL_TYPE" FIELD="CLOSED_LIST"
			NUM_ROW="10">
			<ITEMS DEFAULT="CACHED">
				<ITEM NAME="FIXED" VALUE="FIXED" />
				<ITEM NAME="CACHED" VALUE="CACHED" />
				<ITEM NAME="SCHEDULED" VALUE="SCHEDULED" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="THREAD_POOL_SIZE" FIELD="TEXT" NUM_ROW="10"
			SHOW_IF="(THREAD_POOL_TYPE=='FIXED') OR (THREAD_POOL_TYPE=='SCHEDULED')">
			<DEFAULT>2</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SCHEDULED_TYPE" FIELD="CLOSED_LIST"
			NUM_ROW="30" SHOW_IF="(THREAD_POOL_TYPE=='SCHEDULED')">
			<ITEMS DEFAULT="NORMAL">
